  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "./dist/vue2-ez-common.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "name": "@phamlinh0208/vue2-ez-common",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/npm/cli.git"
  }
